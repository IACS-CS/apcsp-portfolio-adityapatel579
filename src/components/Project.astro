---
import Page from "./Page.astro";

let { title, assignment } = Astro.props;
---

<Page title={title}>
  <div class="container">
    <header>
      <h1>
        Project: <span set:html={title}></span>
      </h1>
      <nav>
        <a href="../../"><i class="fa fa-solid fa-backward"></i></a>
      </nav>
    </header>
    <article>
      <p>
        Assignment: {assignment}
      </p>
      <hr />
      <slot />
    </article>
  </div>
</Page>
